	<!-- Aniade un nuevo capitulo de una nueva temporada a una serie -->
	<category>
	<pattern>aniade la nueva temporada <set>number</set> a ^</pattern>
	<template>
		<think><set name="nomSerie"><star index="2"/></set></think>
		<think><set name="temporada"><star index="1"/></set></think>
		<think><set name="topic">tituli</set></think>
		Dime el titulo del primer capitulo
	</template>
	</category>

	
	<topic name="tituli">

	<!-- Pide el nombre del capitulo -->
	<category>
	<pattern> ^ </pattern>
	<template>
		<think><set name="titulo"><star/></set></think>
		<think><set name="topic">duration</set></think>
		Dime la duracion del primer capitulo
	</template>
	</category>

	</topic>

	<topic name="duration">

	<!-- Pide la duracion del capitulo -->
	<category>
	<pattern>  <set>number</set></pattern>
	<template>
		<think><set name="duracion"><star/></set></think>
		<think><set name="topic">unknown</set></think>
			<think><addtomap>
				<name><get name="nomSerie"/></name>
				<key>temporada<get name="temporada"/></key>
				<value><get name="titulo"/>@<get name="duracion"/>@</value>
			</addtomap></think>
		La nueva temporada ya esta disponible en la plataforma
	</template>
	</category>

	</topic>




















	
	<!-- Aniadir Capitulo -->
	<category>
	<pattern>aniadir un nuevo capitulo a la temporada <set>number</set> de *</pattern>
	<template>
		<think><set name="serie"><star index="2"/></set></think>
		<think><set name="temporada"><star index="1"/></set></think>
		<think><set name="topic">titcap</set></think>
		Dime el titulo del capitulo</template>
	</category>

	
	<topic name="titcap">
	<!-- Pide titulo -->
		<category>
		<pattern> ^ </pattern>
		<template>
			<think><set name="titulo"><star index="1"/></set></think>
			<think><set name="topic">durcap</set></think>
			Dime la duracion del capitulo
		</template>
		</category>
	</topic>
	
	<topic name="durcap">
	<!-- Pide la duracion del capitulo -->
		<category>
		<pattern><set>number</set></pattern>
		<template>
			<think><set var="eltel"><map><name><get name="serie"/></name>temporada<get name="temporada"/></map></set></think>
			<think><set name="duracion"><star/></set></think>
			<think><set name="topic">unknown</set></think>
				<think><modifymap>
					<name><get name="serie"/></name>
					<key>temporada<get name="temporada"/></key>
					<value><get var="eltel"/>@<get name="titulo"/>@<get name="duracion"/></value>
				</modifymap></think>
			Este capitulo ya esta disponible en la serie
		</template>
		</category>
	</topic>















